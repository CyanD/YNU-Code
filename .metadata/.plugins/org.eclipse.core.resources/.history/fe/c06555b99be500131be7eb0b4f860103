package test;

public class test {
	int[] array;

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] c=new int[2];
		test t=new test(c);
		int t;
		c[0]=100;c[1]=5;c[2]=40;

		System.out.println(c[0]);
		System.out.println(c[1]);
		System.out.println(c[2]);
		
	}

	public test(int[] array) {
		super();
		this.array = array;
	}
	public void sort(){
		if(this.array[0]>this.array[1]){
			t=this.array[0];
			this.array[0]=this.array[1];
			this.array[1]=t;
		}
		if(c[1]>c[2]){
			t=this.array[1];
			this.array[1]=this.array[2];
			this.array[2]=t;
		}
		if(c[0]>c[1]){
			t=this.array[0];
			this.array[0]=this.array[1];
			this.array[1]=t;
		}
	}
	

}
