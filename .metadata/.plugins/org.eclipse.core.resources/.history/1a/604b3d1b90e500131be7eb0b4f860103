package test;

public class test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] c=new int[3];
		int t;
		c[0]=100;c[1]=5;c[2]=2;
		if(c[0]>c[1]){
			t=c[0];
			c[0]=c[1];
			c[1]=t;
		}
		if(c[1]>c[2]){
			t=c[1];
			c[1]=c[2];
			c[2]=t;
		}
		if(c[0]>c[1]){
			t=c[0];
			c[0]=c[1];
			c[1]=t;
		}
		System.out.println(c[0]);
		System.out.println(c[1]);
		System.out.println(c[2]);
	}

}
